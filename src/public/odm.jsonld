{
    "@context" : {
        "@version": 1.1, 
        "dc": "http://purl.org/dc/terms/",
        "odm": "http://localhost:4000/odm.jsonld#",
        "data": {"@id": "odm:data"},
        "studyOID": {"@id": "odm:studyOID"},
        "metaDataVersionOID": {"@id": "odm:metaDataVersionOID"},
        "itemGroupData": {
            "@container": "@index",
            "@id": "odm:itemGroupData"
          },
        "items": {
          "@container": "@id",
          "@id": "odm:items"
        },      
        "itemData": {
          "@container": "@list",
          "@id": "odm:itemData"
        },
        "records": {"@id": "odm:records", "@type": "xsd:integer"},
        "name": {"@id": "odm:name"},
        "label": {"@id": "odm:label"},
        "type": {"@id": "odm:type"},
        "length": {"@id": "odm:length", "@type": "xsd:integer"}
    }
}